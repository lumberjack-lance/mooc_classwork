<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lumberjack_lance.band_app" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LinkListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LinkDetailActivity"
            android:label="@string/title_link_detail"
            android:parentActivityName=".LinkListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LinkListActivity" />
        </activity>
    </application>

</manifest>
